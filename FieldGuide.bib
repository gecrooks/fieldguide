	Doi = {10.1073/pnas.1403232111},
	Fullname = {1},
	Journal = {Proc. Natl. Acad. Sci. U.S.A.},
	Pages = {15912--15917},
	Themonth = {11},
	Title = {Scaling laws governing stochastic growth and division of single bacterial cells},
